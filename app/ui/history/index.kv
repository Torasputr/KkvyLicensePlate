<HistoryIndex>:
    BoxLayout:
        orientation: 'vertical'
        BoxLayout:
            size_hint_y: None
            height: 50
            Button:
                text: "Back"
                size_hint_x: None
                width: 100
                on_release: root.manager.current = 'homeIndex'
            Label:
                text: "History"
                font_size: 24
                bold: True
        RecycleView:
            id: history_list
            viewclass: 'Label'
            RecycleBoxLayout:
                padding: 20
                orientation: 'vertical'
                size_hint_y: None
                height: self.minimum_height
